$DISTRIBUTION cauchy

#IO SETTINGS

$outfile 110.dat

#CRYSTAL PARAMETERS

$C_1 fcc.in
$C_2 fcc.in
$f_constants constants.in

#CONFIGURATION PARAMETERS

$Z1 1 1 0
$X1 0 0 1
$Z2 1 1 0
$X2 0 0 1
$ThetaRotZ1 1.
$ThetaRotZ2 0
$theta_min 0
$theta_max 90
$dtheta 2.5

#NUMERICAL PARAMETERS

$tolerance 1E-2
$distribution DISTRIBUTION

$test sin(3.1415926535)

#$vtk
$vtk_rot_x1 55
$vtk_rot_x2 -55
$resolution 50
